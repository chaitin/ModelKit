{
  "name": "@yokowu/modelkit-ui",
  "version": "0.2.0",
  "description": "A reusable AI model configuration modal component for React applications",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --config examples/vite.config.ts",
    "preview": "vite preview --config examples/vite.config.ts",
    "dev:rollup": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@c-x/ui": "^1.0.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@monaco-editor/react": "4.7.0",
    "@mui/icons-material": "^6.4.12",
    "@mui/lab": "6.0.0-beta.19",
    "@mui/material": "^6.4.12",
    "@mui/system": "^7.3.1",
    "ahooks": "^3.8.4",
    "axios": "^1.9.0",
    "dayjs": "^1.11.7",
    "decimal.js": "^10.5.0",
    "echarts": "^5.6.0",
    "lottie-react": "^2.4.1",
    "monaco-editor": "^0.52.2",
    "react": "^18.2.0",
    "react-activity-calendar": "^2.7.12",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@c-x/cx-swagger-api": "^0.0.10",
    "@eslint/js": "^9.25.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/react": "^18.2.0",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "rollup": "^4.46.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "shiki": "^3.7.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "react",
    "ai",
    "model",
    "modal",
    "configuration",
    "openai",
    "ollama",
    "material-ui",
    "typescript"
  ],
  "author": "ModelKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaitin/ModelKit.git"
  },
  "homepage": "https://github.com/chaitin/ModelKit#readme",
  "bugs": {
    "url": "https://github.com/chaitin/ModelKit/issues"
  }
}
