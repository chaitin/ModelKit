{
  "name": "@yokowu/modelkit-ui",
  "version": "0.7.2",
  "description": "A reusable AI model configuration modal component for React applications",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --config examples/vite.config.ts",
    "preview": "vite preview --config examples/vite.config.ts",
    "dev:rollup": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "icon": "node ./scripts/downLoadIcon.cjs"
  },
  "keywords": [
    "react",
    "ai",
    "model",
    "modal",
    "configuration",
    "openai",
    "ollama",
    "material-ui",
    "typescript"
  ],
  "author": "ModelKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaitin/ModelKit.git"
  },
  "homepage": "https://github.com/chaitin/ModelKit#readme",
  "bugs": {
    "url": "https://github.com/chaitin/ModelKit/issues"
  },
  "dependencies": {
    "@c-x/ui": "^1.0.9",
    "@mui/material": "^6.4.12",
    "react": "^19.1.0",
    "react-activity-calendar": "^2.7.12",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.4.1",
    "axios": "^1.11.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  }
}
